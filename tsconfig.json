{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2016",
    "types": [
      "vitest/globals"
    ],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "paths": {
      "@guide-mini-vue/*": [
        "./packages/*/src"
      ]
    },
  },
  "include": [
    "packages/*/src",
    "packages/*/__tests__"
  ]
}